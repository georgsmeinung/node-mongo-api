doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
  body
    nav.navbar.navbar-expand-md.navbar-dark.bg-dark.fixed-top
      a(href="#").navbar-brand= title
      div#navbarsExample.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          li.nav-item
            a(href="/").nav-link Home
          li.nav-item
            a(href="#").nav-link Link
          li.nav-item
            a(href="#").nav-link.disabled Disabled Link
    br
    br
    br
    main(role="main").container
      div.jumbotron
        block content
